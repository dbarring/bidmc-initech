<h1>Listing ctas</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Pi</th>
    <th>Workflow status</th>
    <th></th>
    <th></th>
  </tr>

<% @ctas.each do |cta| %>
  <tr>
    <td><%= link_to cta.name, cta%></td>
    <td><%= User.find(cta.pi_id).full_name %></td>
    <td><%= cta.workflow_status %></td>
    <td><%= link_to 'Edit', edit_cta_path(cta) %></td>
    <td><%= link_to 'Destroy', cta, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Cta', new_cta_path %> <br>
<%= link_to 'Search', action: 'search' %> <br>
<%= link_to 'Admin Panel', controller: 'users', action: 'index' %>
