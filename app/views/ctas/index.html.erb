<h1>Listing ctas</h1>

<table>
  <tr>
    <th>Pi</th>
    <th>Workflow status</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @ctas.each do |cta| %>
  <tr>
    <td><%= cta.pi_id %></td>
    <td><%= cta.workflow_status %></td>
    <td><%= link_to 'Show', cta %></td>
    <td><%= link_to 'Edit', edit_cta_path(cta) %></td>
    <td><%= link_to 'Destroy', cta, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Cta', new_cta_path %>
