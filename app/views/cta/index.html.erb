<h1>Listing cta</h1>

<table>
  <tr>
    <th>Pi</th>
    <th>Workflow status</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @cta.each do |ctum| %>
  <tr>
    <td><%= ctum.pi_id %></td>
    <td><%= ctum.workflow_status %></td>
    <td><%= link_to 'Show', ctum %></td>
    <td><%= link_to 'Edit', edit_ctum_path(ctum) %></td>
    <td><%= link_to 'Destroy', ctum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

