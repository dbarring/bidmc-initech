<h1>Listing forms</h1>

<table>
  <tr>
    <th>Part</th>
    <th>Content</th>
    <th>Cci notes</th>
    <th>Irb note</th>
    <th>Revision child</th>
    <th>Editor</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @forms.each do |form| %>
  <tr>
    <td><%= form.part %></td>
    <td><%= form.content %></td>
    <td><%= form.cci_notes %></td>
    <td><%= form.irb_note %></td>
    <td><%= form.revision_child_id %></td>
    <td><%= form.editor_id %></td>
    <td><%= link_to 'Show', form %></td>
    <td><%= link_to 'Edit', edit_form_path(form) %></td>
    <td><%= link_to 'Destroy', form, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Form', new_form_path %>
